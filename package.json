{
  "name": "better-vsts-reporting",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "npm-run-all": "^4.1.2"
  },
  "engines": {
    "node": "8.11.1"
  },
  "scripts": {
    "build": "cd client && yarn build",
    "start": "node server",
    "start:client": "cd client && yarn start",
    "start:server": "cd server && yarn start",
    "start:dev": "npm-run-all -p start:client start:server",
    "install:dev": "yarn && cd client && yarn && cd ../server && yarn",
    "postinstall": "cd server && npm install"
  },
  "license": "MIT"
}
